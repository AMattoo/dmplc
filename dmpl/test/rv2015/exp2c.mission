#!/bin/bash

DMPL="exp2 AADL.dmpl"
LIBS="dartam/Debug/adaptation_manager.o dartam/Debug/AdaptationPlanner.o"
BIN=exp2

OUTDIR=outdir
MAPNAME=small
GRIDSIZE=10
NODENUM=5

EXPECT_LOG_PERIOD=0.2

MISSION_TIME=115

IX=2
IY=2
FX=7
FY=7

DROP_RATE=`awk -v "seed=$[(RANDOM & 32767) + 32768 * (RANDOM & 32767)]" 'BEGIN { srand(seed); printf("%.5f\n", rand() * 0.2 + 0.4) }'`

echo $DROP_RATE

ARGS_0="--var_x $IX --var_y $IY --var_xt $FX --var_yt $FY --drop-rate $DROP_RATE"
ARGS_1="--var_x $((IX+2)) --var_y $((IY+2)) --drop-rate $DROP_RATE"
ARGS_2="--var_x $((IX-2)) --var_y $((IY+2)) --drop-rate $DROP_RATE"
ARGS_3="--var_x $((IX-2)) --var_y $((IY-2)) --drop-rate $DROP_RATE"
ARGS_4="--var_x $((IX+2)) --var_y $((IY-2)) --drop-rate $DROP_RATE"
